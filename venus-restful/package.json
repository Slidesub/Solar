{
  "name": "venus-restful",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d lib",
    "dev2": "./node_modules/.bin/supervisor --extensions html,node,js --no-restart-on error ./bin/www"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "pug": "^2.0.0-rc.1"
  },
  "devDependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.2.1",
    "koa-jwt": "^3.3.1",
    "log4js": "^2.5.3",
    "mongoose": "^5.0.14",
    "nodemon": "^1.8.1",
    "nunjucks": "^3.1.2",
    "supervisor": "^0.12.0"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015-node5"
        ],
        "plugins": [
          "transform-async-to-generator",
          "syntax-async-functions"
        ]
      }
    }
  }
}
